# Vanna SQL Service Environment Configuration
# Copy this file to .env and fill in your values

# Database Configuration
DATABASE_URL=postgresql://postgres:password@localhost:5433/hrms

# OpenAI Configuration (Required)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-5

# Qdrant Configuration (Optional - uses docker-compose service by default)
QDRANT_URL=http://localhost:6333
QDRANT_COLLECTION=vanna_hrms
QDRANT_API_KEY=



# Service Configuration (Optional - defaults are fine)
# SERVICE_NAME=vanna-sql-service
# SERVICE_VERSION=2.0.0
# HOST=0.0.0.0
# PORT=8010
# LOG_LEVEL=INFO
# SCHEMA_NAME=hrms
# AUTO_TRAIN_ON_STARTUP=true
# CORS_ORIGINS=*
# MAX_QUERY_RESULTS=1000
# QUERY_TIMEOUT=30
